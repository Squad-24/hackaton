@model IEnumerable<TechnicalShare.Models.Mentor>

@{
    ViewData["Title"] = "mentores";
}
<h2>
    @ViewData["Title"]
</h2>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                nome
            </th>
            <th>
                email
            </th>
            <th>
                área de atuação
            </th>   
            <th>
                nível de experiência
            </th>
            <th>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Expertise.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Level)
            </td>

        </tr>
        }
    </tbody>

</table>

