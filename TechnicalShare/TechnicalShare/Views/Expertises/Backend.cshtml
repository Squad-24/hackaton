@model IEnumerable<TechnicalShare.Models.Mentor>

@{
    ViewData["Title"] = "mentores back-end";
}

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Expertise.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Level)
                </td>
                <td>
                    <a asp-action="Profile" asp-route-id="@item.Id">ver perfil</a>
                </td>

            </tr>
        }
    </tbody>
</table>